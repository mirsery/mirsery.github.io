div#about-panel
  - var about_info = theme.about_info;
  div.info
    .title= about_info.name
    a(class='url' href= config.url)= config.url
    .bio= about_info.bio
  img.avatar(src=about_info.avatar)

div#my-music
  iframe( frameborder="no" border="0" marginwidth="0" marginheight="0" width=330 height=370 src="//music.163.com/outchain/player?type=0&id=7097325082&auto=1&height=430") 

div#friend-links-panel
  div.panel-title
    p= __('friend_link')
  - var friend_links = theme.friend_links;
  each link in friend_links
    a.link-item(href=link[2] target="_blank")
        div.link-container
          .site-name
            = link[0] 
          .site-desc= link[1]